package model;


public class Group {

	private String id = "";
	private String groupName = "";

	public String autoGroupGenerateId() {
		String autogenerated;
		int highestInt = 0;

		for (int i = 0; i < Data.getGroupList().size(); i++) {
			if (i == Data.getGroupList().size() - 1) {
				highestInt = Integer.parseInt(Data.getGroupList().get(i)
						.getId().substring(1));
				break;
			}
		}

		autogenerated = "G" + ++highestInt;
		return autogenerated;

	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getGroupName() {
		return groupName;
	}

	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}

}
